  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BabylonJS" Version="2.3.1" />
    <PackageReference Include="Blazor.Bootstrap" Version="3.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="8.0.4" />
